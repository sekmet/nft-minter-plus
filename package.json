  "scripts": {
    "metadata:deploy:ipfs": "node scripts/erc1155_deploy_to_ipfs.js",
    "contract:clean": "hardhat clean && rimraf deployments/ && rimraf lib/contracts.json && rimraf minter_server/src/contracts.json",
    "contract:deploy:rinkeby": "npm run contract:clean && hardhat deploy --network rinkeby --export lib/contracts.json && copyfiles -f lib/contracts.json minter_server/src",
    "near:build": "yarn build:as && yarn build:rs",
    "near:build:as": "node ./contracts/assemblyscript/compile.js",
    "near:build:rs": "(cd contracts/rust && ./build.sh)",
    "near:clean": "rm -rf ./out && (cd contracts/rust && cargo clean)",
    "start": "next start",
    "build": "next build",
    "dev": "next dev",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm run dev",
    "build:tailwind": "tailwind build styles/index.css -o styles/tailwind.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "cids/multicodec": "1.0.1",
    "ipfs-bitswap/multicodec": "0.5.5",
    "**/redis": "3.0.2"
  },
